package com.experian.dto.pojo.litigation;

import java.io.Serializable;
import java.util.List;

import com.experian.dto.entity.litigation.LitigationCaseCategory;
import com.experian.dto.entity.litigation.LitigationCourt;
import com.experian.dto.entity.litigation.LitigationCrawledData;
import com.experian.dto.entity.litigation.LitigationParsedDataDetail;
import com.experian.dto.entity.litigation.LitigationParty;
import com.experian.dto.entity.litigation.LitigationSourceUrl;

@SuppressWarnings("serial")
public class LitigationInfo implements Serializable {
	private LitigationParsedDataDetail detail;

	private List<LitigationParty> party;

	private LitigationCrawledData crawledData;

	private LitigationCourt court;

	private LitigationSourceUrl sourceUrl;

	private LitigationCaseCategory caseCategory;

	public LitigationParsedDataDetail getDetail() {
		return detail;
	}

	public void setDetail(LitigationParsedDataDetail detail) {
		this.detail = detail;
	}

	public List<LitigationParty> getParty() {
		return party;
	}

	public void setParty(List<LitigationParty> party) {
		this.party = party;
	}

	public LitigationCrawledData getCrawledData() {
		return crawledData;
	}

	public void setCrawledData(LitigationCrawledData crawledData) {
		this.crawledData = crawledData;
	}

	public LitigationCourt getCourt() {
		return court;
	}

	public void setCourt(LitigationCourt court) {
		this.court = court;
	}

	public LitigationSourceUrl getSourceUrl() {
		return sourceUrl;
	}

	public void setSourceUrl(LitigationSourceUrl sourceUrl) {
		this.sourceUrl = sourceUrl;
	}

	public LitigationCaseCategory getCaseCategory() {
		return caseCategory;
	}

	public void setCaseCategory(LitigationCaseCategory caseCategory) {
		this.caseCategory = caseCategory;
	}

}

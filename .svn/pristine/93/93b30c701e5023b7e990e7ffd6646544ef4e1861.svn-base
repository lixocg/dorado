	<select id="getCorpLitigationCount" resultType="java.lang.Integer">
		select count(1) from T_CORPLITIGATION
	</select>
	<select id="getCorpLitigationByPage" resultMap="BaseResultMap">
		SELECT
		*
		FROM
		(
		SELECT
		"SBDNUM",
		"INFONUM",
		"OBTAININGDATE",
		"LITIGATIONAGENT",
		"LITIGATION",
		"UPDATETIME",
		"CASECODE",
		"CASEDETAIL",
		"COURT",
		"CASEID",
		"ACCUSER1",
		"ACCUSER2",
		"ACCUSER3",
		"ACCUSER4",
		"ACCUSER5",
		"APPUSER1",
		"APPUSER2",
		"APPUSER3",
		"APPUSER4",
		"APPUSER5",
		"PARTY1",
		"PARTY2",
		"PARTY3",
		"PARTY4",
		"PARTY5",
		"ACCDATE",
		"SIMPLECASE",
		"MINUTECASE",
		"NOTICEDETAILS",
		"NOTE",
		"IFBECOURT",
		"ACCSBDNUM1",
		"ACCSBDNUM2",
		"ACCSBDNUM3",
		"ACCSBDNUM4",
		"ACCSBDNUM5",
		"APPSBDNUM1",
		"APPSBDNUM2",
		"APPSBDNUM3",
		"APPSBDNUM4",
		"APPSBDNUM5",
		"PARTSBDNUM1",
		"PARTSBDNUM2",
		"PARTSBDNUM3",
		"PARTSBDNUM4",
		"PARTSBDNUM5",
		"IFCOLLECT",
		"PKID",
		"COURTID",
		"SINPLECASE",
		"LITIGATIONAGENTURL",
		rownumber () OVER () AS rn
		FROM
		T_CORPLITIGATION T
		) AS a1
		WHERE
		a1.rn BETWEEN #{startRow}
		AND #{endRow}
	</select>
